{
  "rules": [
    {
      "enabled": true,
      "name": "Delete MySQL Backups",
      "type": "Lifecycle",
      "definition": {
        "actions": {
          "baseBlob": {
            "delete": {
              "daysAfterCreationGreaterThan": 30
            }
          }
        },
        "filters": {
          "blobTypes": [
            "blockBlob"
          ],
          "prefixMatch": [
            "mysql-backup/"
          ]
        }
      }
    }
  ]
}
